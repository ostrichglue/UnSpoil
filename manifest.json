{
    "manifest_version": 2,
    "name": "Unspoil",
    "version": "1.0",
  
    "description": "Hides time-based spoilers from YouTube for specified channels.",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["unspoil.js"]
      }
    ]
  }
  